{
  "name": "bsc_generator",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "build:css": "npx tailwindcss -i ./bsc_gen/static/css/tailwind.css -o ./bsc_gen/static/css/tailwind.build.css --minify",
    "tailwind:watch": "npx tailwindcss -c tailwind.config.js -i ./bsc_gen/static/css/tailwind.css -o ./bsc_gen/static/css/tailwind.build.css --watch"
  }
}
